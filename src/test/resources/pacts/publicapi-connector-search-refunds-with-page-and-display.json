{
  "consumer": {
    "name": "publicapi"
  },
  "provider": {
    "name": "connector"
  },
  "interactions": [
    {
      "description": "Return all refunds when refunds exist",
      "providerStates": [
        {
          "name": "Account exists and has 2 refunds"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/v1/refunds/account/777",
        "query": {
          "page": [
            "1"
          ],
          "display_size": [
            "1"
          ]
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "total": 2,
          "count": 2,
          "page": 1,
          "results": [
            {
              "refund_id": "111111",
              "created_date": "2017-10-01T01:41:01Z",
              "refund_summary": {
                "status": "available",
                "amount_available": 98,
                "amount_submitted": 98
              }
            },
            {
              "refund_id": "222222",
              "created_date": "2017-09-02T02:42:02Z",
              "refund_summary": {
                "status": "available",
                "amount_available": 100,
                "amount_submitted": 100
              }
            }
          ],
          "_links": {
            "self": {
              "href": "https://publicapi.example.com/v1/payments/abc123/refunds"
            }
          }
        },
        "matchingRules": {
          "body": {
            "$.total": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.count": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.page": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_id": {
              "matchers": [
                {
                  "regex": "[a-z0-9]*"
                }
              ]
            },
            "$.results[*].created_date": {
              "matchers": [
                {
                  "match": "date",
                  "date": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
                }
              ]
            },
            "$.results[*].status": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.status": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.amount_available": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.amount_submitted": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$._links.self.href": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        }
      }
    }
  ],
  "metadata": {
    "pact-specification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.5.16"
    }
  }
}
