{
  "consumer": {
    "name": "publicapi"
  },
  "provider": {
    "name": "connector"
  },
  "interactions": [
    {
      "description": "Get all refunds",
      "providerStates": [
        {
          "name": "a refund exists"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/v1/refunds",
        "query": {
          "page": [
            "1"
          ],
          "display_size": [
            "1"
          ]
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "total": 1,
          "count": 1,
          "page": 1,
          "results": [
            {
              "refund_id": "123456",
              "created_date": "2018-06-09T02:48:56Z",
              "status": "refund success",
              "refund_summary": {
                "status": "available",
                "amount_available": "123",
                "amount_submitted": "123"
              }
            }
          ],
          "_links": {
            "payment": {
              "href": "https://publicapi.example.com/v1/payments/abc123"
            },
            "self": {
              "href": "https://publicapi.example.com/v1/payments/abc123/refunds"
            }
          }
        },
        "matchingRules": {
          "body": {
            "$.total": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.count": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.page": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_id": {
              "matchers": [
                {
                  "regex": "[a-z0-9]*"
                }
              ]
            },
            "$.results[*].created_date": {
              "matchers": [
                {
                  "match": "date",
                  "date": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"
                }
              ]
            },
            "$.results[*].status": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.status": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.amount_available": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.results[*].refund_summary.amount_submitted": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$._links.payment.href": {
              "matchers": [
                {
                  "regex": "https*:\/\/.*\/v1\/api\/payments\/[a-z0-9]*"
                }
              ]
            },
            "$._links.self.href": {
              "matchers": [
                {
                  "regex": "https*:\/\/.*\/v1\/api\/payments\/[a-z0-9]*\/refunds"
                }
              ]
            }
          }
        }
      }
    }
  ],
  "metadata": {
    "pact-specification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.5.16"
    }
  }
}
